<!--
Se video i bunden af denne side
http://blog.alanklement.com/2009/08/10/fdt-and-ant-a-users-guide-part-i/
--
External tools configuration -> click 'JRE' tab -> Select 'Run in the same JRE as the workspace'.
-->
<project default="compile" name="##PROJECT_NAME##">
	
	<property name="src.dir" value="${basedir}/src/" />
	<property name="deploy.dir" value="${basedir}/bin" />
	
	<target name="compile">
		<fdt.launch.application
			projectname="##PROJECT_NAME##"
			mainclass="${src.dir}/MainClass.as"
			target="${deploy.dir}/main.swf"
			startswf="true"
			swflauncher="External SWF Viewer"/>
	</target>
	
</project>
